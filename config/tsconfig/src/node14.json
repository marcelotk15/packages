{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@teka15/tsconfig-node-14",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
